{
    "name": "API Mocker",
    "version": "1.0",
    "description": "Transfer API docs to Mock.js",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "content_scripts": [
      {
        "matches": ["http://wiki.huitong.com/*"],
        "css": ["mockStyles.css"],
        "js": ["lodash.min.js","mock.min.js","contentScript.js"]
      }
    ],
    "page_action": {
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
  }
